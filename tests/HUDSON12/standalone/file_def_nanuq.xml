          <field field_ref="Ch_ice"          name="Ch_ice"   operation="instant" />
          <field field_ref="Ce_ice"          name="Ce_ice"   operation="instant" />
      -->

      <!-- rheology -->
      <!-- Traditional C-grid location: -->
      <field field_ref="icedmgt"     name="damage-t"    operation="instant" valid_min="0." valid_max="0.999999"    />
      <field field_ref="icedivt"     name="sidive-t"    operation="instant" valid_min="-0.5e-5" valid_max="0.5e-5" />
      <field field_ref="icedeft"     name="sidefo-t"    operation="instant" valid_min="0."      valid_max="1.e-5"  />
      <field field_ref="iceshet"     name="simxshr-t"   operation="instant" valid_min="0."      valid_max="1.e-5"  />
      <!--<field field_ref="icedelt"     name="sidelta-t"   operation="instant" valid_min="0."      valid_max="1.e-5"  />-->
      <!--
          <field field_ref="iceshrf"     name="sishear-f"   operation="instant" valid_min="-0.5e-5" valid_max="0.5e-5" />
          <field field_ref="icevorf"     name="sivort-f"    operation="instant" valid_min="-1.e-5"  valid_max="1.e-5"  />
