<?xml version="1.0"?>

<file_definition type="one_file" name="@expname@_@freq@_@startdate@_@enddate@" sync_freq="6h" min_digits="4">

  <file_group id="1h" output_freq="1h"  output_level="10" >  <!-- 1h files -->

    <file id="file11" name_suffix="_icemod" description="ice variables" enabled=".false." >
      <field field_ref="iceconc"   name="siconc-t"   operation="average" />
      <field field_ref="icevolu"   name="sivolu-t"   operation="average" />
      <field field_ref="uice"      name="u_ice"      operation="average" />
      <field field_ref="vice"      name="v_ice"      operation="average" />
      <field field_ref="iceconc-u" name="siconc-u"   operation="average" />
      <field field_ref="iceconc-v" name="siconc-v"   operation="average" />
    </file>

  </file_group>



