      <!-- -->

      <field field_ref="uice_v"           name="u_ice-v"    operation="instant" valid_min="-0.5" valid_max="0.5" />
      <field field_ref="vice_u"           name="v_ice-u"    operation="instant" valid_min="-0.5" valid_max="0.5" />


      <!-- stress tensors -->
      <!-- T-centric cell -->
      <field field_ref="ice_sig11"   name="sigma11-t"   operation="instant" valid_min="-5.e4" valid_max="1.e4" />
      <field field_ref="ice_sig22"   name="sigma22-t"   operation="instant" valid_min="-5.e4" valid_max="1.e4" />
