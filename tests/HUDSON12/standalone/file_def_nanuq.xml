      <!-- -->

    </file>


    <file id="file32" name_suffix="_flx" description="fluxes open-ocean" enabled=".false." >
      <field field_ref="iceconc"     name="siconc"   operation="average" />
      <field field_ref="ice_cover"                   operation="average" />  <!-- this `fr_i`in SBC stuff, must be same as `siconc` ! -->
      <field field_ref="wspd"                        operation="average" />
      <field field_ref="taum"                        operation="average" valid_min="0."   valid_max="0.5" />
      <field field_ref="qns"                         operation="average" />
      <field field_ref="qsr"                         operation="average" />
      <field field_ref="saltflx"     name="sfx"      operation="average" />
      <field field_ref="fmmflx"                      operation="average" />
      <field field_ref="qt"                          operation="average" />
    </file>


    <file id="file33" name_suffix="_siflx" description="fluxes sea-ice" enabled=".false." >
      <field field_ref="iceconc"     name="siconc"   operation="average" />
      <field field_ref="wspd"        name="wspd"     operation="average" />
      <!--
          <field field_ref="wnd_x"        name="wnd_x"   operation="instant" />
          <field field_ref="wnd_y"        name="wnd_y"   operation="instant" />
      -->
      <!-- flux of momentum -->
      <field field_ref="taum_ai"             operation="average" valid_min="0."   valid_max="0.5" />
      <field field_ref="taux_ai"             operation="average" />
      <field field_ref="tauy_ai"             operation="average" />
      <!-- -->
      <field field_ref="taum_oi"             operation="average" valid_min="0."   valid_max="0.5" />
      <field field_ref="taux_oi_u"           operation="average" />
      <field field_ref="tauy_oi_v"           operation="average" />
      <!-- -->
      <field field_ref="qns_atmo"                     operation="average"  />
      <!-- Air-ice heat fluxes -->
      <field field_ref="qla_ice"    operation="average"  />
      <field field_ref="qt_ice"     operation="average"  />
      <field field_ref="qsr_ice"    operation="average"  />
      <field field_ref="qns_ice"    operation="average"  />
    </file>


    <file id="file34" name_suffix="_flxWA" description="fluxes open ocean surface" enabled=".false." >
      <!-- Open ocean heat fluxes -->
      <!-- flux of momentum between water and air -->
      <field field_ref="iceconc"     name="siconc"       operation="average" />
      <field field_ref="wspd"        operation="average" />
      <field field_ref="rho_air"     operation="average"  />
      <!--  -->
      <field field_ref="taum_wa"     operation="average" valid_min="0."   valid_max="0.5" />
      <field field_ref="qla_wa"      operation="average"  />
      <field field_ref="qsb_wa"      operation="average"  />
      <field field_ref="qlw_wa"      operation="average"  />
      <field field_ref="qns_wa"      operation="average"  />
      <field field_ref="qsr_wa"      operation="average"  />
      <field field_ref="evap_wa"     operation="average"  />
      <field field_ref="utau_wa"     operation="average" />
      <field field_ref="vtau_wa"     operation="average" />
